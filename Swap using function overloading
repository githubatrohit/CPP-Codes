#include<iostream>  
using namespace std;    
void swap(int *x, int *y)//Pointer fx.
{
	int swap;  
	swap=*x;  
	*x=*y;  
	*y=swap;
	  
}  
void swap(int &x, int &y)//Reference
{
	int swap;  
	swap= x;  
	x=y;  
	y=swap;
	
}
void swap(float x,float y)//value 
{
	float swap;
	swap=x;
	x=y;
	y=swap;
	cout<<"value of a is\n "<<x;
	cout<<"value of b is"<<y;
}
int main()   
{    
	int x=500, y=100;
	float a=10.5;
	float b=11.5;    
	swap(&x, &y);//Ptr.
	cout<<"Value of x is: "<<x<<endl;  
	cout<<"Value of y is: "<<y<<endl;
	x=500,y=100;
	swap(x,y); //Refer. 
	cout<<"\nValue of x is: "<<x<<endl;  
	cout<<"\nValue of y is:"<<y<<endl; 
	x=500;y=100;
	swap(a,b);//Value
	cout<<"\nValue of a is:"<<a<<endl;  
	cout<<"\nValue of b is:"<<b<<endl;  
	return 0;  
}    
